# Environment Configuration for Rabobank API Integration

# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Rabobank Developer API Configuration
# Get these from https://developer.rabobank.nl
RABOBANK_CLIENT_ID=your_client_id_here
RABOBANK_CLIENT_SECRET=your_client_secret_here
RABOBANK_API_BASE_URL=https://api.sandbox.rabobank.nl
RABOBANK_OAUTH_URL=https://api.sandbox.rabobank.nl/oauth/v2/authorize

# Certificate paths (for production)
RABOBANK_CERTIFICATE_PATH=./certificates/client.crt
RABOBANK_PRIVATE_KEY_PATH=./certificates/client.key

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here
JWT_EXPIRES_IN=24h

# Database Configuration (if needed for token storage)
DATABASE_URL=sqlite:./database.sqlite

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_URL=https://your-domain.com/api/webhooks/rabobank

# Firebase Configuration (for integration with existing app)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY_ID=your_firebase_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_firebase_private_key\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email
FIREBASE_CLIENT_ID=your_firebase_client_id

# Security
ENCRYPTION_KEY=your_32_character_encryption_key_here
